<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
	<class name="org.azizkhani.model.packet.Passenger" table="AFC_PassengerPacket" lazy="true" >
		<id name="id"  column="ID" >
			<generator class="sequence" />
		</id>
		<property name="actionDate" 				type="string"		column="actionDate" 		  					/>
		<property name="actionTime" 				type="string"		column="actionTime" 							/>
		<property name="amount" 					type="long"			column="amount" 			  					/>
		<property name="cardSerial" 				type="long"			column="cardSerial" 		  					/>
		<property name="reducerTurnRow" 			type="long"			column="reducerTurnRow" 			  			/>
		
		
		<many-to-one name="line" 		column="LineId" 			entity-name="org.azizkhani.model.line.Line" 					 />
		<many-to-one name="driver" 		column="PersonelId" 		entity-name="org.azizkhani.model.personel.Personel" 		 />
		<many-to-one name="bus" 		column="VehicleId" 			entity-name="org.azizkhani.model.vehicle.Vehicle" 			 />
		<many-to-one name="device" 		column="DeviceId" 			entity-name="org.azizkhani.model.device.Device" 			 />
		<many-to-one name="location" 	column="LocationId" 		entity-name="org.azizkhani.model.location.Location" 	 />
        
        
		<property name="createdDate" 	type="date" 	column="CreatedDate" 	not-null="false" update="false" />
		<property name="updatedDate" 	type="date" 	column="UpdatedDate" 	not-null="false" />
	</class>
</hibernate-mapping>