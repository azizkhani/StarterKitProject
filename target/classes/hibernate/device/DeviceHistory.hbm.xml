<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
  <class name="org.baharan.model.device.DeviceHistory" table="AFC_DeviceHistory" lazy="false">
		<id name="id"  column="Id">
			<generator  class="sequence"/>
		</id>
		<property name="simCard" 					type="string" 			column="SimCard" 				not-null="true" length="50" />
		<property name="softwareVersion" 			type="string" 			column="SoftwareVersion" 		not-null="true" length="50" /> 
		<property name="softwareUpdateDateTime" 	type="string" 			column="SoftwareUpdateDateTime" not-null="true" length="50" />
	    <property name="sendDateTime" 				type="string" 			column="SendDateTime" 			not-null="true" length="50" />
		<property name="installDateTime" 			type="string" 			column="InstallDateTime" 		not-null="true" length="50" />
		<property name="historyDateTime" 			type="string" 			column="HistoryDateTime" 		not-null="true" length="50" />
		<property name="status" 					type="boolean" 			column="Status" 				not-null="true"  />
		<property name="description" 				type="string" 			column="Description" 			not-null="true" length="50" />
		
		<many-to-one name="vehicleSetupLocation" entity-name="org.baharan.model.baseInfo.BaseInformation"  >
                <column name="vehicleSetupLocationId" not-null="true" />
        </many-to-one>
        
		<many-to-one name="device" entity-name="org.baharan.model.device.Device"  >
                <column name="DeviceId" not-null="true" />
        </many-to-one>
		
        <many-to-one name="responsible" entity-name="org.baharan.model.personel.Personel"  >
                <column name="ResponsibleId" not-null="false" />
        </many-to-one> 
	
	   	<many-to-one name="location" entity-name="org.baharan.model.location.Location"  >
                <column name="locationId" not-null="true" />
        </many-to-one> 

	   	<many-to-one name="vehicle" entity-name="org.baharan.model.vehicle.Vehicle"  >
                <column name="vehicleId" not-null="false" />
        </many-to-one> 

        <property name="createdBy" 		type="integer" 	column="CreatedBy" 		not-null="true" update="false" />
		<property name="updatedBy" 		type="integer" 	column="UpdatedBy" 		not-null="true" />
		<property name="createdDate" 	type="date" 	column="CreatedDate" 	not-null="false" update="false" />
		<property name="updatedDate" 	type="date" 	column="UpdatedDate" 	not-null="false" />
	</class>
</hibernate-mapping>