<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
	<class name="org.baharan.model.terminal.Terminal" table="AFC_Terminal" lazy="true" >
		<id name="id"  column="ID">
			<generator class="sequence" />
		</id>
		<property name="code" 					type="string"		column="Code" 		  			not-null="true"		unique="true"	/>
		<property name="name" 					type="string"		column="Name" 			  		not-null="true"						/>
		<property name="latitude" 				type="string"		column="Latitude" 			  	not-null="true"						/>
		<property name="longitude" 				type="string"		column="Longitude" 		  											/>
		<property name="lineTotal" 				type="integer"		column="LineTotal" 			  										/>
		<property name="vehicleCapacity" 		type="integer"		column="VehicleCapacity" 	  										/>
		<property name="phoneNumber" 			type="string"		column="PhoneNumber" 		  										/>
		<property name="address" 				type="string"		column="Address"			  										/>
		<property name="description" 			type="string"		column="Description" 		   										/>
		

		<many-to-one name="location" entity-name="org.baharan.model.location.Location"  >
                <column name="LocationId" not-null="true" />
        </many-to-one> 
        <many-to-one name="responsible" entity-name="org.baharan.model.personel.Personel"  >
                <column name="ResponsibleId" not-null="true" />
        </many-to-one> 
       
        
        <property name="createdBy" 		type="integer" 	column="CreatedBy" 		not-null="true" update="false" />
		<property name="updatedBy" 		type="integer" 	column="UpdatedBy" 		not-null="true" />
		<property name="createdDate" 	type="date" 	column="CreatedDate" 	not-null="false" update="false" />
		<property name="updatedDate" 	type="date" 	column="UpdatedDate" 	not-null="false" />
	</class>
</hibernate-mapping>